{
  "name": "stable-diffusion-rest-api",
  "version": "0.0.0",
  "description": "A CLI for running Stable Diffusion locally via a REST API on an M1/M2 MacBook",
  "keywords": [
    "ai",
    "api",
    "cli",
    "image",
    "image-generation",
    "rest-api",
    "stable-diffusion"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/stable-diffusion-rest-api.git"
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib"
  ],
  "bin": {
    "stable-diffusion-rest-api": "./lib/cli.js"
  },
  "scripts": {
    "build": "rm -rf lib && tsc --build",
    "clean": "rm -rf lib input.png output.png",
    "fix": "eslint --fix 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "reset": "rm -rf node_modules package-lock.json && npm install",
    "start": "node --loader ts-node/esm src/cli.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "kleur": "^4.1.5",
    "multer": "^1.4.5-lts.1",
    "sade": "^1.8.1",
    "temp-dir": "^2.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/multer": "^1.4.7",
    "eslint": "^8.23.0",
    "eslint-config-yuanqing": "^0.0.6",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "eslintConfig": {
    "extends": "eslint-config-yuanqing"
  },
  "prettier": "eslint-config-yuanqing/prettier"
}
